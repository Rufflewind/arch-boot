[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = blue
[core]
	excludesfile = ~/.gitignore_global
	# filemode = false # Windows only
[diff]
	tool = kdiff3
[merge]
	stat = true
[push]
	default = simple
[rebase]
	missingCommitsCheck = error
	stat = true
[submodule]
	fetchJobs = 4
